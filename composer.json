{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": ["laravel", "framework"],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "akaunting/laravel-money": "^4.0",
        "awcodes/filament-tiptap-editor": "^3.0@beta",
        "filament/filament": "^3.0.1",
        "filament/notifications": "^3.0",
        "filament/spatie-laravel-media-library-plugin": "^3.0",
        "guzzlehttp/guzzle": "^7.2",
        "iotronlab/laravel-page-view": "^1.0",
        "jeffgreco13/filament-breezy": "^2.1",
        "khanamiryan/qrcode-detector-decoder": "^2.0",
        "laravel/framework": "^10.10",
        "laravel/sanctum": "^3.2",
        "laravel/tinker": "^2.8",
        "razorpay/razorpay": "^2.8.6",
        "simplesoftwareio/simple-qrcode": "^4.2"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.8",
        "doctrine/dbal": "^3.6",
        "fakerphp/faker": "^1.9.1",
        "filament/upgrade": "3.0-stable",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.18",
        "laravel/telescope": "^4.16",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.0",
        "phpunit/phpunit": "^10.1",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan filament:upgrade"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
